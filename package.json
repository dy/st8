{
  "name": "st8",
  "version": "0.1.10",
  "description": "Control state of any object",
  "main": "index.js",
  "dependencies": {
    "polymer-weakmap": "*",
    "enot": "*",
    "mutypes": "*",
    "each-csv": "*"
  },
  "devDependencies": {
    "chai": "~1.9.1",
    "mocha": "~1.21.4",
    "require-stub": "*"
  },
  "scripts": {
    "build": "npm run min-index && browserify index.min.js --standalone st8 > dist/st8.js && npm run minify && npm test",
    "test": "mocha --harmony",
    "minify": "ccjs dist/st8.js --compilation_level=SIMPLE --language_in=ECMASCRIPT5_STRICT > dist/st8.min.js",
    "min-index": "ccjs index.js --language_in=ECMASCRIPT5_STRICT --compilation_level=ADVANCED > index.min.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dfcreative/st8"
  },
  "keywords": [
    "state",
    "fsm"
  ],
  "author": "Deema Evanov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dfcreative/st8/issues"
  }
}
